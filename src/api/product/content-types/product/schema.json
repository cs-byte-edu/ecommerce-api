{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "price_formula": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::price-formula.price-formula",
      "inversedBy": "products"
    },
    "price_variables": {
      "type": "json"
    },
    "final_price": {
      "type": "decimal"
    },
    "attributes": {
      "type": "component",
      "repeatable": true,
      "component": "product.attribute"
    },
    "filter_values": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::filter-value.filter-value",
      "mappedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "product"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "description": {
      "type": "text"
    },
    "brand": {
      "type": "string"
    },
    "category_main": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    }
  }
}
