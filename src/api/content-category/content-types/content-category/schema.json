{
  "kind": "collectionType",
  "collectionName": "content_categories",
  "info": {
    "singularName": "content-category",
    "pluralName": "content-categories",
    "displayName": "Content category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "category_name"
    },
    "category_description": {
      "type": "text"
    },
    "content_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content-item.content-item",
      "mappedBy": "content_category"
    }
  }
}
